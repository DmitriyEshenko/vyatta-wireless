type: u32
help: Set wireless channel
syntax:expression: $VAR(@) > 0 && $VAR(@) < 15; "Channel must be between 1 and 14"
allowed: if [ -n "$VAR(../physical-device)" ]
	 then ${vyatta_bindir}/vyatta-show-wireless.pl --list-chan $VAR(../physical-device)
	 fi

update: if [ "$VAR(../type)" != "access-point" ]
	then sudo iw dev $VAR(../@) set channel $VAR(@)
	fi

comp_help: possible completions
<1-14>	Channel (not all maybe available depending on geography and hardware)
